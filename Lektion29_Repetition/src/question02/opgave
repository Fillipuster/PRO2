// Opgave 02:
// Ret Application.java så programmet kan compile uden compile-time fejl.

package question02;

public class Application {

	public static void main(String[] args) {
		
		Human peter = new Human("Peter", "Justesen", 10000);
		peter.communicate();
		
		Animal robomaster = new Robot("Robomaster");
		robomaster.communicate();
		
		Human torsk = new Fish("Torsk", 23);
		torsk.breathe();
		
		Animal dog = new Animal("Bailey", 142);
		dog.excreteWaste();
		
		Animal ørred = new Fish("Ørred", 2);
		ørred.breathe();
		
		ørred = new Human("Jonas", "Præstegaard", 8050);
		ørred.breathe();
		
	}
	
}
